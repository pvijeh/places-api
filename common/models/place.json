{
  "name": "Place",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "Name": {
      "type": "string"
    },
    "Id": {
      "type": "number",
      "id": true,
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "users":{
      "type" : "hasMany",
      "model" : "AppUser", 
      "foreignKey" : "PlaceId", 
      "through" : "Vote" 
    },
      "votes":{
      "type" : "hasMany",
      "model" : "Vote", 
      "foreignKey" : "PlaceId" 
    }
    },
  "acls": [],
  "methods": {}
}
